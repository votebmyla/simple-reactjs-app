pipeline {
  agent any
  environment {
    DOCKERHUB_CREDENTIALS=credentials('dockerhub')
  }
  stages {
    stage('listing directory'){
      steps{
        sh 'ls -l'
      }
    }
    stage('build'){
      steps{
        sh 'npm install'
        sh 'npm run build'
        sh 'ls -l'
      }
    }
  }
}